use stdlib/io;
exp fn info(s string) -> void {
  println("[?] " + s);
}

exp fn err(s string) -> i32 {
  println("\e[31m[!]\e[0m " + s);
  return 1;
}

exp fn success(s string) -> void {
  println("\e[32m[+]\e[0m " + s);
}

exp fn panic(s string) -> void {
  println("\e[91m[!!!]\e[0m " + s);
  exit(1);
}

exp fn warn(s string) -> void {
  println("\e[33m[~]\e[0m " + s);
}
